{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ConfigResponse",
  "type": "object",
  "required": [
    "blockClaim",
    "blockIcoTimeframe",
    "blockPlay",
    "claimReward",
    "claimTicket",
    "delegatorMinAmountInDelegation",
    "denom",
    "denomDelegation",
    "denomShare",
    "drandGenesisTime",
    "drandPeriod",
    "drandPublicKey",
    "everyBlockHeight",
    "holdersRewards",
    "owner",
    "players",
    "tokenHolderSupply",
    "validatorMinAmountToAllowClaim"
  ],
  "properties": {
    "blockClaim": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "blockIcoTimeframe": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "blockPlay": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "claimReward": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/CanonicalAddr"
      }
    },
    "claimTicket": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/CanonicalAddr"
      }
    },
    "delegatorMinAmountInDelegation": {
      "$ref": "#/definitions/Uint128"
    },
    "denom": {
      "type": "string"
    },
    "denomDelegation": {
      "type": "string"
    },
    "denomShare": {
      "type": "string"
    },
    "drandGenesisTime": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "drandPeriod": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "drandPublicKey": {
      "$ref": "#/definitions/Binary"
    },
    "everyBlockHeight": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "holdersRewards": {
      "$ref": "#/definitions/Uint128"
    },
    "owner": {
      "$ref": "#/definitions/CanonicalAddr"
    },
    "players": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/CanonicalAddr"
      }
    },
    "tokenHolderSupply": {
      "$ref": "#/definitions/Uint128"
    },
    "validatorMinAmountToAllowClaim": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    }
  },
  "definitions": {
    "Binary": {
      "description": "Binary is a wrapper around Vec<u8> to add base64 de/serialization with serde. It also adds some helper methods to help encode inline.\n\nThis is only needed as serde-json-{core,wasm} has a horrible encoding for Vec<u8>",
      "type": "string"
    },
    "CanonicalAddr": {
      "$ref": "#/definitions/Binary"
    },
    "Uint128": {
      "type": "string"
    }
  }
}
